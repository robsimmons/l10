// Adding negation to regular expressions

neg:  regexp -> regexp.          

token _ I, 
token _ J, 
I <= J, 
not (match RE I J) -> 
   match (neg RE) I J.

db3 = (token "d" 0, token "a" 1, 
       token "a" 2, token "EOF" 3) 
  @ w1 (seq (neg (alt (tok "b") (tok "c")))
         (some (tok a))).

db4 = (token "b" 0, token "a" 1, 
       token "a" 2, token "EOF" 3) 
  @ w1 (seq (neg (alt (tok "b") (tok "c")))
         (some (tok a))).
