tree: type.
leaf: nat -> tree.
cons: tree -> tree -> tree.

w0: nat -> world.
w1: tree -> world.

w1 (cons X Y) <- w1 X, w1 Y.
w1 (leaf N) <- w0 N.

true: rel @ w0 0.
db0 = true @ w1 (leaf 1).
db1 = true @ w1 (cons (cons (leaf 1) (leaf 2)) (cons (leaf 3) (leaf 4))).
db2 = true @ w1 (cons (cons (leaf 1) (leaf 2)) (cons (leaf 1) (leaf 3))). 